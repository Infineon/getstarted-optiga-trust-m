        					<GridData horizontalSpan="2"/>
        				</p1:GCheck.layoutData>
        			</p1:GCheck>
        			<p1:GLabel manifestObj="true" text="   OSC_HP clock source:" widgetName="glabel_osc_hp_source"/>
        			<p1:GCombo mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="External Crystal&#13;&#10;External Direct Input" value="External Crystal" widgetName="gcombo_osc_hp_source" description="This allows to select clock source for OSC_HP. The clock source can be derived from: &lt;br&gt;&#13;&#10;1. External Crystal Oscillator.&lt;br&gt;&#13;&#10;2. External Direct Input.&lt;br&gt;"/>
        			<p1:GLabel manifestObj="true" text="   OSC_HP clock frequency [MHz]:" toolTipText="" widgetName="glabel_extclkfreq"/>
        			<p1:GFloat description="This is used to configure External Crystal Oscillator (OSC_HP) or External Direct Input (OSC_HP) clock frequency value." format="*,3" manifestObj="true" maxValue="20.0" minValue="4.0" toolTipText="Configure External Crystal Oscillator or External Direct Input&#13;&#10;clock frequency value." value="20.0" widgetName="gfloat_extclkfreq" x:Style="BORDER">
        				<p1:GFloat.layoutData>
        					<GridData widthHint="125"/>
        				</p1:GFloat.layoutData>
        			</p1:GFloat>
        			<Label/>
        			<Label/>
        			<p1:GCheck manifestObj="true" text="Enable Low Precision Oscillator (OSC_LP)" widgetName="gcheck_enable_osc_lp" description="This selection enables Low Precision Oscillator (OSC_LP). It is used as a clock source for RTC and also used for DCO1 clock frequency calibration during runtime to achieve a better accuracy." toolTipText="This selection enables Low Precision Oscillator (OSC_LP). &#13;&#10;It is used as a clock source for RTC and also used for DCO1&#13;&#10;clock frequency calibration during runtime to achieve a &#13;&#10;better accuracy.">
