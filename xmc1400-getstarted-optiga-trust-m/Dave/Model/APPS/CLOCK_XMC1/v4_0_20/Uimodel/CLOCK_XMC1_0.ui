        			</p1:Tab.layout>
                    <p1:GLabel text="Clock control unit source:" visible="true" widgetName="glabel_ccuclksrc"/>
                    <p1:GCombo description="This allows to select clock source for clock control unit to generate MCLK and PCLK." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="DCO1&#13;&#10;OSC_HP" toolTipText="Select clock source for clock control unit to generate MCLK and PCLK." value="DCO1" widgetName="gcombo_ccuclksrc">
                    	<p1:GCombo.layoutData>
                    		<GridData horizontalAlignment="FILL"/>
                    	</p1:GCombo.layoutData>
                    </p1:GCombo>
                    <Label/>
                    <Label/>
                    <p1:GLabel text="Main clock (MCLK) [MHz]:" toolTipText="Enter Desired Main Clock Frequency - MCLK,&#13;&#10;Valid clock range is 125 kHz to 32000 kHz." visible="true" widgetName="glabel_mclkdesfrq"/>
                    <p1:GFloat description="This allows to configure desire MCLK (Main Core Clock) clock frequency value.&#13;&#10;MCLK is a source of many module clocks (except CCU4/8, POSIF, MATH, BCCU), and it is derived from the dco_dclk oscillator. Dividers are derived using the following formula:&lt;br&gt;&#13;&#10;&lt;b&gt;MCLK = (dco_dclk / (2 * (IDIV + FDIV/256) ) ),&lt;/b&gt; where IDIV always &gt; 0." format="" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="32.0" minValue="0.125" toolTipText="Configure desire MCLK (Main Core Clock) clock frequency value." value="32.0" widgetName="gfloat_mclkdesfrq" x:Style="BORDER">
                    	<p1:GFloat.layoutData>
                    		<GridData horizontalAlignment="FILL"/>
                    	</p1:GFloat.layoutData>
                    </p1:GFloat>
                    <p1:GLabel text="Actual setting [MHz]:" toolTipText="Actual MCLK" visible="true" widgetName="glabel_mclkactfrq"/>
